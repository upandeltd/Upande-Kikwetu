  "format_data": null,
  "html": "<link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css\" rel=\"stylesheet\"\n    integrity=\"sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH\" crossorigin=\"anonymous\">\n\n<div class=\"\">\n    <div class=\"row justify-content-between\">\n        <div class=\"col-auto mr-auto\">\n            <div>\n                <div class=\"alert alert-info text-center\">\n                    <h6 style=\"margin: 0px;\">{{ doc.status }}</h6>\n                </div>\n\n\n                {%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n                {% if letter_head and not no_letterhead %}\n                <div class=\"letter-head\">{{ letter_head }}</div>\n                {% endif %}\n\n                {% if max_pages > 1 %}\n                <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n                {% endif %}\n                {%- endmacro -%}\n\n                {{ add_header(0, 1, doc, letter_head, no_letterhead) }}\n            </div>\n            <br>\n        </div>\n\n        <div class=\"col-auto\">\n            <h6>PURCHASE INVOICE</h6>\n            <p>Reference Number<br>{{ doc.name }}</p>\n            {% if doc.items and doc.items[0].purchase_order %}\n            <p>\n                <strong>Purchase Order:</strong> {{ doc.items[0].purchase_order }}\n            </p>\n            {% endif %}\n\n            {% if doc.items and doc.items[0].purchase_receipt %}\n            <p>\n                <strong>Purchase Receipt:</strong> {{ doc.items[0].purchase_receipt }}\n            </p>\n            {% endif %}\n            <label>Date: </label>\n            <p>{{ doc.posting_date or '' }}</p>\n            <label>Due Date: </label>\n            <p>{{ doc.due_date or '' }}</p>\n        </div>\n    </div>\n</div>\n\n<br>\n\n<div class=\"row mb-3\">\n    <div class=\"col-md-12\">\n        <label>Supplier Information: </label>\n        <p>{{ doc.supplier_name or '' }}</p>\n    </div>\n\n</div>\n\n<br>\n\n<table class=\"table table-bordered\">\n    <tbody>\n        <tr>\n            <th style=\"width: 25%; text-align: left;\">Item</th>\n            <th style=\"width: 38%; text-align: left;\">Description</th>\n            <th style=\"width: 10%; text-align: center;\">Qty</th>\n            <th style=\"width: 10%; text-align: center;\">UOM</th>\n            <th style=\"width: 15%; text-align: center;\">Unit Price</th>\n            <th style=\"width: 15%; text-align: center;\">Tax</th>\n            <th style=\"width: 15%; text-align: center;\">Discount</th>\n            <th style=\"width: 15%; text-align: center;\">Amount</th>\n\n        </tr>\n        {%- for row in doc.items -%}\n        <tr>\n            <td style=\"text-align: left;\">{{ row.item_code }}</td>\n            <td style=\"text-align: left;\">{{ row.item_code }}</td>\n            <td style=\"text-align: center;\">{{ row.qty }}</td>\n            <td style=\"text-align: center;\">{{ row.uom }}</td>\n            <td style=\"text-align: right;\">{{ frappe.utils.fmt_money(row.rate) }}<div style=\"border: 0px;\"></div>\n            </td>\n            <td style=\"width: 10%; text-align: left;\">{{ row.item_tax_template }}</td>\n            <td class=\"text-end\">\n                {% if row.discount_percentage %}\n                {{ row.discount_percentage }}%\n                {% elif row.discount_amount %}\n                {{ frappe.utils.fmt_money(row.discount_amount, currency=doc.currency) }}\n                {% else %}\n                -\n                {% endif %}\n            </td>\n            <td style=\"text-align: right;\">{{ frappe.utils.fmt_money(row.amount) }}</td>\n\n        </tr>\n        {%- endfor -%}\n    </tbody>\n</table>\n\n<!-- Totals -->\n<div class=\"container-fluid mt-4\">\n    <div class=\"row\">\n      <div class=\"col-md-6\"></div> <!-- Left side left blank -->\n      <div class=\"col-md-6 d-flex flex-column align-items-start p-4 border rounded\">\n  \n        {% set total = namespace(value=0) %}\n        {% for item in doc.items %}\n        {% set total.value = total.value + (item.qty * item.rate) %}\n        {% endfor %}\n  \n        <!-- Gross (Item Total) -->\n        <p class=\"d-flex justify-content-between w-100\">\n          <span>Gross Total</span>\n          <span>{{ frappe.utils.fmt_money(total.value, currency=doc.currency) }}</span>\n        </p>\n  \n        <!-- Total Amount (Net Total) -->\n        <p class=\"d-flex justify-content-between w-100\">\n          <span>Total Amount</span>\n          <span>{{ doc.get_formatted(\"net_total\", doc) }}</span>\n        </p>\n  \n        <!-- Tax Withholding -->\n        {% if doc.tax_withholding_amount and doc.tax_withholding_amount != 0 %}\n        <p class=\"d-flex justify-content-between w-100\">\n          <span>Tax Withholding</span>\n          <span>-{{ doc.get_formatted(\"tax_withholding_amount\", doc) }}</span>\n        </p>\n        {% endif %}\n  \n        <!-- VAT -->\n        {% for tax in doc.taxes %}\n        {% if \"VAT\" in tax.account_head or \"VAT\" in tax.description %}\n        <p class=\"d-flex justify-content-between w-100\">\n          <span>VAT</span>\n          <span>{{ frappe.utils.fmt_money(tax.tax_amount, currency=doc.currency) }}</span>\n        </p>\n        {% endif %}\n        {% endfor %}\n  \n        <!-- Additional Discount -->\n        {% if doc.discount_amount and doc.discount_amount != 0 %}\n        <p class=\"d-flex justify-content-between w-100\">\n          <span>Additional Discount</span>\n          <span>-{{ doc.get_formatted(\"discount_amount\", doc) }}</span>\n        </p>\n        {% endif %}\n  \n        <!-- Grand Total -->\n        <p class=\"d-flex justify-content-between w-100 fw-bold\">\n          <span>Grand Total</span>\n          <span>{{ doc.get_formatted(\"grand_total\", doc) }}</span>\n        </p>\n  \n        <hr class=\"w-100 my-2\"> <!-- Divider -->\n  \n        <!-- Rounded Total -->\n        <p class=\"d-flex justify-content-between w-100 fs-5\">\n          <span>Rounded Total</span>\n          <span>{{ doc.get_formatted(\"rounded_total\", doc) }}</span>\n        </p>\n  \n        <!-- Amount Paid -->\n        {% if doc.paid_amount and doc.paid_amount != 0 %}\n        <p class=\"d-flex justify-content-between w-100\">\n          <span>Amount Paid</span>\n          <span>{{ doc.get_formatted(\"paid_amount\", doc) }}</span>\n        </p>\n        {% endif %}\n  \n        <!-- Amount Due -->\n        {% if doc.outstanding_amount and doc.outstanding_amount != 0 %}\n        <p class=\"d-flex justify-content-between w-100\">\n          <span>Amount Due</span>\n          <span>{{ doc.get_formatted(\"outstanding_amount\", doc) }}</span>\n        </p>\n        {% endif %}\n  \n      </div>\n    </div>\n  </div>\n  ",
