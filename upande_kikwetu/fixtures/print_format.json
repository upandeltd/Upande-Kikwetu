  "css": "    .label-container {\n        width: 140mm;\n        height: auto; \n        line-height: 1;\n        border: 4px solid black;\n        font-family: Arial, sans-serif;\n        text-transform: uppercase;\n        font-weight: bold;\n    }\n    \n    .customer-label {\n        font-size: 45px;\n        font-weight: 900;\n        line-height: 1;\n    }\n    \n    .customer-p-o {\n        font-size: 30px;\n    }\n    \n    .label-section .label {\n        font-size: 26px;\n    }\n\n    .label-header {\n        text-align: center;\n        font-size: 18px;\n        font-weight: bold;\n    }\n    \n    .label-value {\n        padding-right: 5px;\n        font-size: 20px;\n    }\n    \n    .label {\n        margin: 0;\n        border: 0;\n    }\n\n    .label-section {\n        display: flex;\n        justify-content: space-between;\n        border-top: 2px solid black;\n        padding: 5px 0;\n        font-size: 14px;\n    }\n\n    .label-section span {\n        font-weight: bold;\n    }\n\n    .product-list {\n        font-size: 20px;\n        font-weight: bold;\n        margin-top: 5px;\n        padding-right: 5px;\n    }",
