  "html": "<link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css\" rel=\"stylesheet\"\n    integrity=\"sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH\" crossorigin=\"anonymous\">\n\n<div class=\"container\">\n    <!-- Row for Letterhead and Purchase Order Details -->\n    <div class=\"row mb-3\">\n        <!-- Letterhead on the left -->\n        <div class=\"col-auto\">\n            {%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n            {% if letter_head and not no_letterhead %}\n            <div class=\"letter-head\">{{ letter_head }}</div>\n            {% endif %}\n\n            {% if max_pages > 1 %}\n            <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n            {% endif %}\n            {%- endmacro -%}\n\n            {{ add_header(0,1,doc,letter_head, no_letterhead) }}\n        </div>\n\n        <!-- Purchase Order Details on the right with margin-right -->\n        <div class=\"col-auto ms-auto\">\n            <div>\n                <div class=\"alert alert-info text-center\">\n                    <h6 style=\"margin: 0px;\">{{ doc.status}}</h6>\n                </div>\n            </div>\n\n            <!-- Purchase Order Details with margin-right -->\n            <div class=\"row mb-3\">\n                <div class=\"col-md-12 text-end\">\n                    <h6>PURCHASE ORDER</h6>\n                    <p><strong>Purchase Order Date</strong><br>{{ doc.get_formatted(\"transaction_date\") or ''\n                        }}<br><br><strong>Purchase Order Number</strong><br>{{ doc.name }}</p>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <!-- Supplier Information Section -->\n    <div class=\"row mb-3\">\n        <div class=\"col-md-12\">\n            <strong>Supplier:</strong>\n            <p>{{ doc.supplier_name }}<br>{% if doc.tax_id %}PIN: {{ doc.tax_id }} {% endif %}</p>\n        </div>\n    </div>\n\n    <!-- Warehouse Address and PO Status -->\n    <div class=\"row mb-3\">\n        <!-- Warehouse Address on the left -->\n        <div class=\"col-md-6\">\n            <strong>Warehouse Address:</strong>\n            {%- if doc.custom_warehouse_address -%}\n            <p>{{ doc.custom_warehouse_address | replace(\"\\n\", \"<br>\") }}</p>\n            {%- else -%}\n            <p>No Address Available</p>\n            {%- endif -%}\n        </div>\n\n        <!-- PO Status on the right with margin-right -->\n        <div class=\"col-md-6 d-flex justify-content-end\">\n            <div>\n                <strong>Approval Status:</strong>\n                <p>{{ doc.workflow_state or '' }}</p>\n            </div>\n        </div>\n    </div>\n</div>\n\n<table class=\"table table-bordered\">\n    <tbody>\n        {% set taxable = {'amount_origin': 0, 'tax':0} %}\n        {% set non_taxable = {'amount_origin': 0, 'tax':0} %}\n        {% set discount = {'total': 0} %}\n        <tr>\n            <th>Item Code</th>\n            <th>Description</th>\n            <th class=\"text-align: left;\">Qty</th>\n            <th class=\"text-align: right;\">UOM</th>\n            <th class=\"text-align: right;\">Rate</th>\n            <th class=\"text-align: right;\">Tax</th>\n            <th class=\"text-align: right;\">Gross</th>\n        </tr>\n        {%- for row in doc.items -%}\n        <tr>\n            <td style=\"width: 20%; text-align: left;\">{{ row.item_name }}</td>\n            <td style=\"width: 25%; text-align: left;\">{{ row.description }}</td>\n            <td style=\"width: 10%; text-align: left;\">{{ row.qty }}</td>\n            <td style=\"width: 10%; text-align: left;\">{{ row.uom }}</td>\n            <td style=\"width: 10%; text-align: left;\">{{ row.rate }}</td>\n            <td style=\"width: 10%; text-align: left;\">{{ row.item_tax_template }}</td>\n            <td style=\"width: 10%; text-align: left;\">{{ row.amount }}</td>\n        </tr>\n        {%- endfor -%}\n    </tbody>\n</table>\n\n<!-- Totals -->\n<div class=\"container-fluid mt-4\">\n    <div class=\"row\">\n        <div class=\"col-md-6\"></div> <!-- Left side left blank -->\n        <div class=\"col-md-6 d-flex flex-column align-items-start p-4 border rounded\">\n\n            {% set total = namespace(value=0) %}\n            {% for item in doc.items %}\n            {% set total.value = total.value + (item.qty * item.rate) %}\n            {% endfor %}\n\n            <!-- Gross (Item Total) -->\n            <p class=\"d-flex justify-content-between w-100\">\n                <span>Gross Total</span>\n                <span>{{ frappe.utils.fmt_money(total.value, currency=doc.currency) }}</span>\n            </p>\n\n            <!-- Total Amount (Net Total) -->\n            <p class=\"d-flex justify-content-between w-100\">\n                <span>Total Amount</span>\n                <span>{{ doc.get_formatted(\"net_total\", doc) }}</span>\n            </p>\n\n            <!-- Tax Withholding -->\n            {% if doc.tax_withholding_amount and doc.tax_withholding_amount != 0 %}\n            <p class=\"d-flex justify-content-between w-100\">\n                <span>Tax Withholding</span>\n                <span>-{{ doc.get_formatted(\"tax_withholding_amount\", doc) }}</span>\n            </p>\n            {% endif %}\n\n            <!-- VAT (Assuming it's part of Taxes) -->\n            {% for tax in doc.taxes %}\n            {% if \"VAT\" in tax.account_head or \"VAT\" in tax.description %}\n            <p class=\"d-flex justify-content-between w-100\">\n                <span>VAT</span>\n                <span>{{ frappe.utils.fmt_money(tax.tax_amount, currency=doc.currency) }}</span>\n            </p>\n            {% endif %}\n            {% endfor %}\n\n            <!-- Additional Discount -->\n            {% if doc.discount_amount and doc.discount_amount != 0 %}\n            <p class=\"d-flex justify-content-between w-100\">\n                <span>Additional Discount</span>\n                <span>-{{ doc.get_formatted(\"discount_amount\", doc) }}</span>\n            </p>\n            {% endif %}\n\n            <!-- Grand Total -->\n            <p class=\"d-flex justify-content-between w-100 fw-bold\">\n                <span>Grand Total</span>\n                <span>{{ doc.get_formatted(\"grand_total\", doc) }}</span>\n            </p>\n\n            <hr class=\"w-100 my-2\"> <!-- Divider -->\n\n            <!-- Rounded Total -->\n            <p class=\"d-flex justify-content-between w-100 fs-5\">\n                <span>Rounded Total</span>\n                <span>{{ doc.get_formatted(\"rounded_total\", doc) }}</span>\n            </p>\n\n        </div>\n    </div>\n</div>",
